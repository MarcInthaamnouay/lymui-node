{
  "name": "lymui-node",
  "version": "1.1.1a",
  "description": "Lymui-node is a small color conversion library for NodeJS",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "install": "node installer.js && node-gyp rebuild",
    "test": "./node_modules/mocha/bin/mocha tests/*.test.js",
    "test-ts": "./node_modules/mocha/bin/mocha -r ts-node/register tests/ts/*.test.ts",
    "build:dev": "node-gyp build --debug",
    "build": "node-gyp build",
    "rebuild:dev": "node-gyp rebuild --debug",
    "rebuild": "node-gyp rebuild",
    "clean": "node-gyp clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcInthaamnouay/lymui-node.git"
  },
  "author": "marc intha-amnouay <android_gs2@hotmail.fr>",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/MarcInthaamnouay/lymui-node/issues"
  },
  "homepage": "https://github.com/MarcInthaamnouay/lymui-node#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "mocha": "^5.2.0",
    "segfault-handler": "^1.0.1",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.1"
  },
  "keywords": [
    "color",
    "conversion",
    "async",
    "converter",
    "rgb",
    "hsl",
    "xyz",
    "lch",
    "luv",
    "hsv",
    "color-converter",
    "color-conversion"
  ],
  "dependencies": {
    "@types/node": "^10.12.20",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "request": "^2.88.0",
    "unzipper": "^0.9.10"
  }
}
